{
    "$id": "https://example.com/array-test.schema.json",
    "$schema": "https://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "objectArray": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "testNumber": {
                        "type": "number"
                    }
                },
                "required": ["testNumber"]
            }
        },
        "primitiveArray": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": ["objectArray", "primitiveArray"]
}
